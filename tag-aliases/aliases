alias gp='git push'
alias gpu='git pull'

alias 'pgstart=pg_ctl -D /opt/boxen/data/postgresql-9.3/ -l logfile start'
alias 'pgstop=pg_ctl -D /opt/boxen/data/postgresql-9.3/ stop'
alias 'pgrestart=pg_ctl -D /opt/boxen/data/postgresql-9.3/ restart'

alias 'mongo=mongo --port 17017'

alias 'nom=rm -rf node_modules && npm cache clean && npm install'

